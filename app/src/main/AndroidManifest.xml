<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expertfeedwoof">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:theme="@style/AppTheme2"
            android:name="com.example.expertfeedwoof.RegisterActivity"
            android:parentActivityName="com.example.expertfeedwoof.MainActivity" >

        </activity>
        <activity
            android:name="com.example.expertfeedwoof.HomeActivity"
            android:parentActivityName="com.example.expertfeedwoof.MainActivity" >

        </activity>

        <activity
            android:theme="@style/AppTheme2"
            android:name="com.example.expertfeedwoof.DetailPesanan"
            android:parentActivityName="com.example.expertfeedwoof.HomeActivity" >

        </activity>

        <activity
            android:name="com.example.expertfeedwoof.MessageActivity"
            android:parentActivityName="com.example.expertfeedwoof.DetailPesanan" >

        </activity>

        <activity
            android:name="com.example.expertfeedwoof.InboxActivity"
            android:parentActivityName="com.example.expertfeedwoof.HomeActivity" >

        </activity>

    </application>

</manifest>